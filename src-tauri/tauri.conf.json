{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "kalendar-malaysia",
    "version": "0.4.2"
  },
  "tauri": {
    "allowlist": {
      "all": false
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "hafizhaziq.dev.kalendar-malaysia",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "height": 650,
        "width": 700,
        "minHeight": 650,
        "minWidth": 700,
        "resizable": true,
        "title": "kalendar-malaysia",
        "center": true
      }
    ]
  }
}
